<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vsa.yogaforlife">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Home" />
        <activity
            android:name=".YogaList"
            android:label="Yoga"
            android:parentActivityName=".Home" />
        <activity
            android:name=".MediList"
            android:label="Meditation"
            android:parentActivityName=".Home" />
        <activity
            android:name=".NearList"
            android:label="Institutions"
            android:parentActivityName=".Home" />
        <activity
            android:name=".Description"
            android:parentActivityName=".YogaList" />
        <activity android:name=".Splash" />
        <activity
            android:name=".DescriptionMedi"
            android:parentActivityName=".MediList" />
        <activity android:name=".Help" />
        <activity android:name=".Feedback"></activity>
    </application>

</manifest>